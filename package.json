{
  "name": "kafka-sink-postgres",
  "version": "1.0.0",
  "description": "kafka-sink-postgres",
  "main": "index.js",
  "scripts": {
    "migration1": "node_modules/.bin/sequelize db:migrate --config /infrastructure/postgres/postgres1.js --migrations-path /infrastructure/postgres/migrations",
    "migration2": "node_modules/.bin/sequelize db:migrate --config /infrastructure/postgres/postgres2.js --migrations-path /infrastructure/postgres/migrations",
    "migrations": "npm run migration1; npm run migration2",
    "seeder1": "node_modules/.bin/sequelize db:seed:all --config /infrastructure/postgres/postgres1.js --seeders-path /infrastructure/postgres/seeders ",
    "seeders": "npm run seeder1"
  },
